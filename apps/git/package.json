{
  "name": "git-service",
  "version": "1.0.0",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun"
  },
  "dependencies": {
    "@repo/types": "workspace:*",
    "@repo/storage": "workspace:*",
    "bun": "latest"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
